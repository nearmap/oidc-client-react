{
  "name": "oidc-client-react",
  "version": "0.1.0-alpha.6",
  "description": "A react wrapper for oidc-client",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:md": "remark -i .gitignore --no-stdout --use remark-lint *.md",
    "jest": "jest --runInBand --no-cache --verbose --config ./jest.config.json",
    "lint": "run-s -s lint:*",
    "clean-build": "run-s clean build",
    "build": "run-s clean build:* pkg:*",
    "build:lib": "babel --ignore *.test.js --source-maps --out-dir build/pkg src",
    "pkg:pkg": "cp package.json build/pkg/",
    "pkg:html": "cp src/silent_renew.html build/pkg/",
    "pkg:docs": "cp README.md build/pkg/",
    "test": "run-s -s lint jest",
    "preci": "npm --loglevel=warn install",
    "ci": "run-s -s clean test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearmap/oidc-client-react.git"
  },
  "keywords": [
    "react",
    "redux",
    "oidc-client",
    "OpenId-Connect"
  ],
  "author": "Jan Klaas Kollhof <jan@kollhof.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nearmap/oidc-client-react/issues"
  },
  "homepage": "https://github.com/nearmap/oidc-client-react#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0-rc.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.2",
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.2",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0-rc.2",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-rc.2",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0-rc.2",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.2",
    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-react": "^7.0.0-rc.2",
    "@babel/runtime": "^7.0.0-rc.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.0.2",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.4.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.11.1",
    "jest-cli": "^23.5.0",
    "jwt-decode": "~2.2.0",
    "npm-run-all": "~4.1.2",
    "prop-types": "~15.6.1",
    "react": "~16.4.0",
    "react-dom": "~16.4.0",
    "react-test-renderer": "~16.4.0",
    "redux": "~4.0.0",
    "remark-cli": "~5.0.0",
    "remark-lint": "~6.0.1"
  },
  "dependencies": {
    "oidc-client": "~1.5.2"
  },
  "peerDependencies": {
    "react": "~16.4.0",
    "react-dom": "~16.4.0",
    "prop-types": "~15.6.1"
  }
}
